#include<stdio.h>
#include<conio.h>
void main()
{
 int i,j;
 clrscr();
 for(i=0;i<5;i++)
 {
	for(j=0;j<5;j++)
	{
	 if(j==0  ||i==0 ||(i==1 && j==4)||(i==2 && j==2)||(i==2 && j==3)||(i==2 && j==4)||(i==3 && j==3)||(i==4 && j==4))
		 printf("*");
	else
		printf(" ");
	}
	printf("\n");

 }
    getch();
}
